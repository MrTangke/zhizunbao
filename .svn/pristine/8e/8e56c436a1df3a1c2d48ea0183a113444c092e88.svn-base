.mycenter_box {
    padding: 30px 0;
}

.list_left {
    width: 248px;
    height: auto;
    overflow: hidden;
    float: left;
    border: 1px solid #e0e0e0;
    background: #fff;
}

.list_left ul li {
    background: url("../images/center/bor.jpg") repeat-x bottom;
    overflow: hidden;
}

.list_left ul li.head {
    text-align: center;
    padding-left: 0;
    padding-top: 34px;
    padding-bottom: 16px;
    font-size: 16px;
}

.list_left ul li.head img {
    display: block;
    margin: 0 auto;
}

.list_left ul li.head div,
.list_left ul li.head .tit {
    display: block;
    font-size: 16px;
    height: 30px;
    padding-top: 10px;
    overflow: hidden;
    color: #333;
    border-bottom: 0;
    padding-left: 0;
}

.list_left ul li.head .count {
    padding-left: 18px;
    color: #ff6700;
    background: url("../images/center/jifen.png") no-repeat left center;
}

.list_left ul li h3 {
    padding-left: 84px;
    color: #3d3d3d;
    font: bold 14px/30px SimSun, "宋体", Arial, Helvetica, sans-serif;
    height: 30px;
    overflow: hidden;
    border-bottom: 1px solid #e0e0e0;
    padding-top: 11px;
    padding-bottom: 12px;
    cursor: pointer;
}

.list_left ul li h3.order {
    background: url("../images/center/order.png") no-repeat 47px 11px;
}

.list_left ul li h3.per {
    background: url("../images/center/per.png") no-repeat 47px 11px;
}

.list_left ul li h3.wallet {
    background: url("../images/center/wallet.png") no-repeat 47px 11px;
}

.list_left ul li div {
    font: 12px/30px SimSun, "宋体", Arial, Helvetica, sans-serif;
    padding: 0 0 0 47px;
    color: #515151;
}

.list_left ul li div ul li {
    height: 30px;
    overflow: hidden;
    border-bottom: 1px solid #e0e0e0;
    border-top: 0;
    padding-top: 11px;
    padding-bottom: 12px;
    background: none;
    cursor: pointer;
    margin-left: 34px;
    margin-bottom: -1px;
}

.list_left ul li div ul li.active, .list_left ul li div ul li.active a {
    color: #ff6700;
    font-weight: bold;
}

.list_left ul li div ul li.active a,
.list_left ul li div ul li a {
    display: block;
}

.mycenter_right {
