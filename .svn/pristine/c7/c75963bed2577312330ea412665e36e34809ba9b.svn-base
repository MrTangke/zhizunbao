/*common*/
.main_width{
    width:1200px;
    margin-left: auto;
    margin-right: auto;
}
/*健康告知*/
.container{
    margin-top: 10px;
    margin-bottom: 20px;
    padding-bottom: 20px;
    background: #fff;
    border:1px solid #e3e3e3;
}
.health_notify h1{
    font: 30px/40px "Microsoft YaHei";
    color: #515151;
    text-align: center;
    padding: 30px 0;
}
.procedures{
    height:auto;
    overflow: hidden;
    text-align: center;
    color: #fff;
    margin-bottom: 10px;
}
.notify_box{
    width:926px;
    height:auto;
    overflow: hidden;
    margin:0 auto;
    padding-bottom: 50px;
}
.notify_box h2{
    background: url("../images/book/fill08.jpg") no-repeat left center;
    height:33px;
    overflow: hidden;
    padding-left: 50px;
    font:18px/33px "Microsoft YaHei";
    color: #515151;
    padding-bottom: 18px;
    border-bottom: 2px dashed #b7b5b5;
    margin-bottom: 10px;
}
.notify_box p{
    padding: 0 30px;
    height:auto;
    overflow: hidden;
    font-size: 14px;
    line-height: 21px;
}
.notify_box p.special{
    font-size: 18px;
}
.notify_box .health_box{
    color: #515151;
    margin-top: 50px;
}
.notify_box a{
    width:150px;
    border:1px solid #ccc;
    border-radius: 5px;
    display: inline-block;
    height:40px;
    margin-right: 15px;
    float: left;
    color: #515151;
    text-align: center;
    font:14px/42px "Microsoft YaHei";
}
.notify_box .job{
    width:150px;
    height:40px;
    line-height:40px;
    display: block;
    text-align: center;
    font-size: 14px;
    border-radius: 5px;
    color: #fff;
    background: #ff9600;
    margin:12px 0;
    cursor: pointer;
    float: none;
}
.notify_box .health_box a.active{
    border:1px solid #ff9600;
}
.notify_box a:hover,
.notify_box a:active{
    background: #ff9600;
    border:1px solid #ff9600;
    color: #fff;
}
/*填写保单*/
.container{
    margin-top: 10px;
    padding-bottom: 55px;
    background-color: #fff;
}
.book_box{
    width:926px;
    height:auto;
    overflow: hidden;
    margin-left: auto;
    margin-right: auto;
    margin-top: 10px;
    border:1px solid #e3e3e3;
}
.book_box h1{
    font:30px/40px "Microsoft YaHei";
    color: #515151;
    text-align: center;
    padding: 30px 0;
}
.book_box .procedures{
    height:auto;
    overflow: hidden;
    text-align: center;
    /*color: #fff;*/
    margin-bottom: 10px;
}
.book_box .verificationBox h2{
    font:bold 18px/26px "Microsoft YaHei";
    color: #515151;
    padding: 7px 0 7px 14px;
    margin-left: 34px;
    margin-right: 34px;
    background: #f0f0f0;
}
.book_box .verificationBox .resetMess{
    display: inline-block;
    height:24px;
    border:1px solid #ff9600;
    border-radius: 5px;
    font: 10px/24px "Microsoft YaHei";
    color: #ff9600;
    padding-left: 23px;
    padding-right: 7px;
    background: #fff url("../images/book/s01.jpg") no-repeat 7px center;
    float: right;
    margin-right: 9px;
}
.verificationBox form{
    margin-left: 46px;
    height:auto;
    padding-bottom: 28px;
}
.verificationBox form p{
    height:28px;
    font:16px/28px "Microsoft YaHei";
    color: #515151;
    margin-top: 20px;
    position: relative;
}
.verificationBox form p label{
    line-height:28px;
    width:102px;
    display: inline-block;
    float: left;
}
.verificationBox form p .input{
    height:26px;
    border:1px solid #c4c4c4;
    width:280px;
    border-radius: 5px;
    position: relative;
    display: inline-block;
    padding-left: 5px;
}
.verificationBox form p .center{
    text-align: center;
}
.verificationBox form p.birth_date select{
    width:68px;
}
.verificationBox form p.birth_date span{
    font-size: 12px;
    margin:0 6px;
}
.subBtn{
    width:280px;
    height: 40px;
    font:18px/40px "黑体","Microsoft YaHei";
    color: #fff;
    background: #ff9600;
    display: inline-block;
    margin: 40px 0 20px 323px;
    text-align: center;
    outline:none;
    border:none;
    cursor: pointer;
}
/*保单确认*/
.container{
    background: #fff;
    overflow: hidden;
}
.container h1{
    font:30px/40px "Microsoft YaHei";
    color: #515151;
    text-align: center;
    padding: 30px 0;
}
.container .procedures{
    text-align: center;
    margin-bottom: 10px;
}
.container .con_detail{
    width:838px;
    height:auto;
    overflow: hidden;
    margin:0 auto;
    border:1px solid #e3e3e3;
    padding: 38px;
}
.container .con_detail p.jwx_zhifutishi{
    color: #FF2C2D;
    text-indent: 44px;
    position: relative;
    top:-10px;
    height:19px;
    line-height:19px;
}
.container .con_detail p.jwx_zhifutishi2{
    top:0;
    margin:10px 0 20px 0;
    text-align: right;
}
.container .con_detail p.jwx_zhifutishi b{
    position: absolute;
    left:18px;
    top:0;
    width:19px;
    height: 19px;
    background: url("../images/book/jwx_zhifutishi.png") no-repeat;
}
.container .con_detail p.jwx_zhifutishi2 b{
    display: inline-block;
    position: relative;
    left:-5px;
    top:4px;
}
.container .con_detail h2{
    height:24px;
    overflow: hidden;
    border-left: 5px solid #ff9600;
    padding-left: 15px;
    font:bold 20px/24px "Microsoft YaHei";
    color: #515151;
    margin-top: 15px;
}
.container .con_detail ul{
    height:auto;
    padding-bottom: 12px;
    padding-top: 15px;
    margin-top: 15px;
    border-top: 1px solid #dbdbdb;
}
.container .con_detail ul li{
    padding-left: 24px;
    font:14px/26px "Microsoft YaHei";
    color: #515151;
}
.container .con_detail h3{
    height:48px;
    font:bold 18px/48px "Microsoft YaHei";
    color: #515151;
    padding-top: 5px;
    padding-left: 24px;
}
.container .con_detail .total{
    height:auto;
    overflow: hidden;
    margin-top: 20px;
}
.container .con_detail .total p{
    text-align: right;
    font: 14px/24px "Microsoft YaHei";
    color: #515151;
}
.container .con_detail .total p .wprice{
    width:80px;
    display: inline-block;
    text-align: right;
}
.total p em, .total p span{
    color: #d94a42;
}
.payment a{
    font:14px/35px "Microsoft YaHei";
    width:100px;
    height:35px;
    display: inline-block;
    text-align: center;
    color: #fff;
    background: #ff9600;
    margin-left: 15px;
}
.payment span, .payment em, .payment a{
    float: right;
}
/*支付方式*/
.paybox_main{
    width:870px;
    margin:0 auto;
    padding: 20px 0 80px;
    color: #333;
    background-color: #fff;
}
.paybox_main .titlebox{
    padding: 20px 0 30px;
    font-size: 18px;
}
.paybox_main .titlebox em{
    color: #ff3126;
}
.paybox_main .titlebox h2{
    padding-left: 100px;
    text-indent: -99px;
    margin-right: 200px;
    font-size: 18px;
}
.paybox_main .tabbox{
    height:36px;
    margin-top: 20px;
    border-bottom: 1px solid #ff9600;
}
.paybox_main .tabbox li{
    float: left;
    width:180px;
    height:36px;
    line-height:36px;
    padding: 1px 1px 0;
    font-size: 16px;
    text-align: center;
    cursor: pointer;
}
.paybox_main .tabbox li.on{
    padding: 0;
    color: #ff9600;
    border:solid #ff9600;
    border-width:1px 1px 0;
    background-color: #fff;
}
.paybox_main .conbox{
    padding: 20px 0;
}
.paybox_main .conbox .con{
    display: none;
}
.paybox_main .conbox .active{
    display: block;
}
.paybox_main .conbox .paylist{
    width: 906px;
}
.paybox_main .conbox .paylist li{
    position: relative;
    z-index:1;
    width: 188px;
    height:38px;
    line-height:38px;
    border:1px solid #d3d3d3;
    float: left;
    font-size: 16px;
    text-align: center;
    margin:10px 36px 0 0;
    cursor: pointer;
    background: url("../images/book/bank-logo.png") no-repeat top center;
}
.paybox_main .conbox .paylist li.on{
    border-color: #ff9600;
}

.paybox_main .conbox .paylist li i{
    position: absolute;
    bottom: 0;
    right:0;
    z-index: 1;
    width:0;
    height:0;
    font-size: 0;
    overflow: hidden;
    border-width: 6px;
    border-style: dashed solid solid dashed;
    border-color: transparent #ff9600 #ff9600 transparent;
    display: none;
}
.paybox_main .conbox .paylist li.on i{
    display: block;
}
.paybox_main .conbox .paylist li.alipay{
    background-position: center 0;
}
.paybox_main .conbox .paylist li.wechat{
    background-position: center -40px;
}
.paybox_main .conbox .paylist li.yinlian{
    background-position: center -80px;
}
.paybox_main .conbox .paylist li.CMB{
    background-position: center -120px;
}
.paybox_main .conbox .paylist li.ICBCB2C{
    background-position: center -160px;
}
.paybox_main .conbox .paylist li.BOCB2C{
    background-position: center -200px;
}
.paybox_main .conbox .paylist li.CCB{
    background-position: center -120px;
}
.paybox_main .conbox .paylist li.CMB{
    background-position: center -240px;
}
.paybox_main .conbox .paylist li.ABC{
    background-position: center -280px;
}
.paybox_main .conbox .paylist li.COMM{
    background-position: center -320px;
}
.paybox_main .conbox .paylist li.SPDB{
    background-position: center -360px;
}
.paybox_main .conbox .paylist li.CMBC{
    background-position: center -400px;
}
.paybox_main .conbox .paylist li.POSTGC {
    background-position: center -440px;
}
.paybox_main .paybtn{
    padding: 30px 0;
}
.paybox_main .paybtn .btn{
    float: left;
    width:200px;
    height:50px;
    line-height:50px;
    text-align: center;
    color: #fff;
    font-size: 24px;
    background-color: #ff9600;
    border-radius: 2px;
    margin-right: 20px;
}
.paybox_main .paybtn .error{
    line-height:50px;
    color: #d94a42;
    display:block;
}
.paybox_main .ansBox{
    color: #999;
    padding-top: 134px;
}
.paybox_main .ansBox h4{
    line-height: 40px;
    font-size: 18px;
    border-bottom:1px dashed #999999;
}
.paybox_main .ansBox h4 a{
    float: right;
    color: #999;
    padding-left: 26px;
    background: url("../images/book/service-icon.png") no-repeat left 7px;
}
.paybox_main .ansBox ol{
    padding-left: 22px;
}
.paybox_main .ansBox ol li{
    padding-top: 30px;
    list-style: decimal;
}
.paybox_main .ansBox ol li p{
    line-height: 30px;
    font-size: 16px;
}